<?xml version="1.0"?>
<parameters>

    <!-- Connection details -->
    <type>YUGABYTE</type>
    <driver>com.yugabyte.Driver</driver>
    <url>jdbc:yugabytedb://localhost:5433/yugabyte?sslmode=disable&amp;ApplicationName=featurebench&amp;reWriteBatchedInserts=true</url>
    <username>yugabyte</username>
    <password></password>
    <isolation>TRANSACTION_REPEATABLE_READ</isolation>
    <batchsize>128</batchsize>
    <microbenchmark>
        <class>com.oltpbenchmark.benchmarks.featurebench.customworkload.CopyCommandBenchmark</class>
        <properties>
            <csvpath>path/to/csv</csvpath>
            <table>sbtest</table>
        </properties>
    </microbenchmark>


    <benchmark>
        <csvpath>/path/to/the/file</csvpath>

    </benchmark>
    <!-- This parameter has no affect on this benchmark-->
    <!-- There is no data to load -->
    <scalefactor>1</scalefactor>

    <!-- The workload -->
    <terminals>1</terminals>
    <works>
        <work>
            <time>60</time>
            <rate>1000</rate>
            <weights>100</weights>
        </work>
    </works>

    <transactiontypes>
        <transactiontype>
            <name>FeatureBench</name>
        </transactiontype>
    </transactiontypes>
</parameters>
